                        <div class="help-block" data-ng-show="frmFiltro.cedula.$error.uicedulanozero">
                            <i class="icon-scytl-error"></i>Cédula inválida
                        </div>
                    </div>
                </div>
                <div class="control-group big ng-class: {error:frmFiltro.nombres.$error.maxlength}">
                    <label class="control-label">Nombres:</label>
                    <div class="controls">
                        <input type="text" class="input-large" name="nombres" data-ng-model="modeloReporte5.nombres" data-ng-maxlength="100">
                        <div class="state-required" data-ng-show="frmFiltro.nombres.$error.maxlength"><i class="icon-scytl-error"></i>Ingrese como máximo 100 caracteres</div>
                    </div>
                </div>
                <div class="control-group big ng-class: {error:frmFiltro.apellidos.$error.maxlength}">
                    <label class="control-label">Apellidos:</label>
