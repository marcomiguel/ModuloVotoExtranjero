														<li data-ng-show="adjuntos.dactilares.izquierda.descripcionImpedimentoHuella1!==''&&adjuntos.dactilares.izquierda.descripcionImpedimentoHuella1!==null">{{adjuntos.dactilares.izquierda.descripcionImpedimentoHuella1}}</li>
														<li data-ng-show="adjuntos.dactilares.izquierda.descripcionImpedimentoHuella2!==''&&adjuntos.dactilares.izquierda.descripcionImpedimentoHuella2!==null">{{adjuntos.dactilares.izquierda.descripcionImpedimentoHuella2}}</li>
													</ul>
												</div>
												<div class="fss"><b>Detalle</b>
													<p>{{adjuntos.dactilares.izquierda.detalle}}</p>
												</div>
											</div>
										</div>
									</div>
									<div class="span6">
										<div id="huella_derecha" class="dinline-block mrl">
