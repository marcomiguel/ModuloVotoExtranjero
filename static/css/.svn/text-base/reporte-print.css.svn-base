@charset "utf-8";
body{
	background-color:white;
	color:black;
    margin:0.5cm 1cm 0;
	font-family:Arial, Helvetica, sans-serif;
	line-height: 1.2;
	overflow: auto;
	font-size:8pt;
	/*width:595px;*/ /* A4 */
	/*height:842px;*/ /* A4 */
}
.preload-print, .btn-print{ display:none!important }
table{ max-width:100%; font-family:Arial, Helvetica, sans-serif; border:1px solid #000; border-spacing:0; border-collapse:collapse; padding:0; margin:0 }
td, th{ border:1px solid #000; padding:2pt; margin:0;  }
#wrap, .wrap { 
	padding:0; margin:0;
}
@page{
   /*size: landscape;*/
    /*size: A4 portrait;*/ 
    margin:0.5cm 0;
	/*margin: 0;
	orphans : 0;
	widows : 0;*/
   	overflow: hidden;
   	border:0cm;
	zoom: 100%;
}
.page-break { height:0; page-break-before:always; margin:0; border-top:none; }
.title-main{ margin:0 0 10px; line-height:1; padding: 0; font-size:12pt }
.sub-title-main{ margin:0 0 10px; line-height:1; padding: 0; font-size:10pt }
a:link:after, a:visited:after { content:" [" attr(href) "] "; }
.separacion{ height:10px; width:100%; overflow:hidden; clear:both }
.sinrelleno { border: 0 none !important }
.sinrelleno td, .sinrelleno th { border: 0 none !important; padding: 0; }
.mt50 { margin:0 }
#configGrid{ display:none }