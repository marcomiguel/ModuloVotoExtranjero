@charset "utf-8";
body{
	background-color:white;
	color:black;
	padding:0!important;
	margin:0;
	font-size:12px;
	font-family:Arial, Helvetica, sans-serif;
	line-height: 1.2;
	position:relative;
	overflow: hidden
}
.preload-print{ position:fixed; left:0; top:0; z-index:9; display:block;background:#fff url('../img/icon-processing.gif') 48% 100px no-repeat;margin:0;height:100%;width:100%;}
table{ width:100%; font-family:Arial, Helvetica, sans-serif; border:1px solod #000; border-spacing:0; border-collapse:collapse; padding:0; margin:0 }
td, th{ border:1px solid #000; padding:4px 5px; margin:0 }
.wrap { padding: 0; margin:0 }
.page-break	{ height:10px; overflow:hidden; border-top:1px dotted #999; margin-bottom:10px; margin-top:15px }
.btn-print{ position:fixed; z-index:9; display:block; background:#fff; margin:0;height:100%;width:100%; text-align:center }
.btn-print .btn-blue{
	margin:100px auto 0;
	padding:10px 15px;
	font-size:16px;
	color: #fff;
	cursor:pointer;
	display:inline-block
}
.btn-blue{
	background-image: -moz-linear-gradient(top,#455b80 0,#455b80 100%);
	background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0%,#455B80),color-stop(100%,#455B80));
	background-image: -webkit-linear-gradient(top,#455B80 0,#455B80 100%);
	background-image: -o-linear-gradient(top,#455b80 0,#455b80 100%);
	background-image: -ms-linear-gradient(top,#455b80 0,#455b80 100%);
	background-image: linear-gradient(to bottom,#455B80 0,#455B80 100%);
	background-color: #455B80;
	color: #FFF;
	border: 1px solid #3F577F;
	text-shadow: 1px 1px 0 #20376F;
	box-shadow: none;
}
.btn-blue:hover,.btn-blue:focus{
	background-color:#3a4d6c!important;
	background-image:none!important;
}
[class^="icon-"], [class*=" icon-"] {
	display: inline-block;
	background-image: url('../img/icons-sprite.png');
	background-repeat: no-repeat;
	margin-right: 5px;
	margin-left: 0;
	height: 16px;
	width: 16px;
	vertical-align: text-bottom;
}
i.icon-scytl-print, i.icon-scytl-print:hover { background-position: -32px -200px; }
.sinrelleno { border: 0 none !important }
.sinrelleno td, .sinrelleno th { border: 0 none !important; padding: 0 none }
.borderdown{ border-bottom:1px solid #000; margin-bottom:5px }
/* alertas */
.alert{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box;position:relative;width:98%;margin:0 auto 20px;padding:8px 30px 8px 30px;border-radius:0;border:solid 1px}
.alert>i{position:absolute;top:9px;left:8px;background-image:url('../img/icons-sprite.png');background-repeat:no-repeat;margin-right:5px;height:16px;width:16px}.alert>button.close{position:absolute;top:10px;right:8px;width:16px;height:16px;background:url('../img/icons-sprite.png') no-repeat -50px -100px;font-size:0;opacity:1}
.alert p{margin-bottom:0; margin-top:3px }
.alert.alert-info{background:#dfecf7;border-color:#cae7f1;color:#5b85ad}
.alert.alert-info i,.alert.alert-info i:hover{background-position:0 0}
.alert.alert-info .close{background-position:-32px -100px}
.alert.alert-success{background:#e4f0d6;border-color:#dce9c3;color:#3c7a70}
.alert.alert-success i,.alert.alert-success i:hover{background-position:-16px 0}
.alert.alert-success .close{background-position:-48px -100px}
.alert.alert-error{background:#ebdddd;border-color:#e5d2d6;color:#a14c47}
.alert.alert-error i,.alert.alert-error i:hover{background-position:-48px 0}
.alert.alert-error .close{background-position:-64px -100px}
.alert.alert-warning{background:#fbf8e1;border-color:#f7eed2;color:#b3974e}
.alert.alert-warning i,.alert.alert-warning i:hover{background-position:-32px 0}
.alert.alert-warning .close{background-position:-80px -100px}
.control-group .alert{margin-bottom:0}
/* config */
#configGrid{ position:absolute; left:0; top:0; z-index:8; width:100%; height:100%; background:white }
#configGrid ul{ list-style:none; margin:0; padding:0; width:880px }
#configGrid ul li{ line-height:1; display:inline-block; overflow:hidden; width:200px; margin:0; min-height:20px; padding:5px 10px }

#configGrid ul li label{ margin:0; display:block; position:relative; padding-left: 20px; min-height: 20px; line-height:1; vertical-align:middle; cursor:pointer }
#configGrid ul li input{ position:absolute; left:0; top:0; z-index:2; margin:0; padding:0 }

#configGrid h4{ font-size:14px; margin:0 0 10px; padding:0 10px; line-height:1 }
#configGrid .alert{ margin:10px auto!important }
#configGrid .btn-print{ margin-top:10px; padding-top:10px; margin-left:10px; border-top:1px solid #E0E1E3 ; height:45px; position:static }
#configGrid .btn-print .btn-blue{ margin:0 auto }